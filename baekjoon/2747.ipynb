   "source": [
    "n = int(input())\n",
    "array = [0, 1]\n",
    "count = 1\n",
    "while True:\n",
    "    if n <= 1:\n",
    "        print(array[n])\n",
    "        break\n",
    "    elif count < n:\n",
    "        array.append(array[-1]+array[-2])\n",
    "        count += 1\n",
    "    else:\n",
    "        print(array[-1])\n",
    "        break"
