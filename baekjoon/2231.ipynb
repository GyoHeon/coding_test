   "source": [
    "n = int(input())\n",
    "\n",
    "def bunhae(n):\n",
    "    k = str(n)\n",
    "    for i in range(len(k)):\n",
    "        n += int(k[i])\n",
    "    return n\n",
    "\n",
    "for i in range(n):\n",
    "    if bunhae(i) == n: \n",
    "        print(i)\n",
    "        break\n",
    "    if i == n-1:\n",
    "        print(0)"
