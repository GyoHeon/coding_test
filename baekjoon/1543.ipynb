   "source": [
    "raw = input()\n",
    "word = input()\n",
    "\n",
    "count = 0\n",
    "for i in range(len(raw)//len(word)):\n",
    "    if word in raw:\n",
    "        count += 1\n",
    "        # aabb, ab 인 경우 반례가 나와서 공백이 아니라 *로 바꿈\n",
    "        raw = raw.replace(word,'*', 1)\n",
    "    else:\n",
    "        break\n",
    "print(count)"
   ],
