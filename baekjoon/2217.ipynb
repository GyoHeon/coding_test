   "source": [
    "n = int(input())\n",
    "li = []\n",
    "for i in range(n):\n",
    "    r = int(input())\n",
    "    li.append(r)\n",
    "li.sort()\n",
    "weight = li[0]*len(li)\n",
    "for j in range(len(li)):\n",
    "    new_weight=li[j]*(len(li)-j)\n",
    "    if new_weight>weight:\n",
    "        weight = new_weight\n",
    "print(weight)"
