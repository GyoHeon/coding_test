   "source": [
    "n = int(input())\n",
    "people = []\n",
    "for i in range(n):\n",
    "    age, name = input().split()\n",
    "    people.append([int(age), i, name])\n",
    "\n",
    "people.sort(key = lambda x : (x[0], x[1]))\n",
    "\n",
    "for i in people:\n",
    "    print(i[0], i[2])"
